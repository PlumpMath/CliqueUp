  <script>
      var map;
      function poll(args) {
          navigator.geolocation.getCurrentPosition(success);
          function success(position) {
              var lat = position.coords.latitude;
              var long = position.coords.longitude;
              initialize(lat, long);
          }
      }

      function initialize(lat, long) {
          var mapOptions = {
              zoom: 10,
              center: new window.google.maps.LatLng(lat, long),
              mapTypeId: window.google.maps.MapTypeId.ROADMAP
          };
          map = new window.google.maps.Map(document.getElementById('map-canvas'),
              mapOptions);


      }

      function addMarkerToMap(mapObject, latitude, longitude) {
          var coordinates = new window.google.maps.LatLng(latitude, longitude);
          var marker = new window.google.maps.Marker({
              position: coordinates,
              map: mapObject
          });
      }

      window.google.maps.event.addDomListener(window, 'load', poll);

      
  </script>
